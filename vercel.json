{
    "version": 2,
    "public": true,
    "name": "webtcvn",
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "https://webtcvn-production.up.railway.app/api/$1"
        },
        {
            "source": "/(.*)",
            "destination": "/frontend/$1"
        }
    ],
    "builds": [
        {
            "src": "frontend/package.json",
            "use": "@vercel/static-build",
            "config": {
                "distDir": "build"
            }
        }
    ]
}